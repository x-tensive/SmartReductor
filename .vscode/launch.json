    "version": "0.2.0",
    "configurations": [
        {
            "name": "Node: Launch SmartReductor",
            "program": "${workspaceFolder}/build/smartReductor.js",
            "args": [ 
                "import",
                "enterpriseStruct",
                "--url", "http://smartreductor.francecentral.cloudapp.azure.com",
                "--user", "admin",
                "--password", "admin"
            ],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "console": "externalTerminal"
        }
    ]
}